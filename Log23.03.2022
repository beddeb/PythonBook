from itertools import *

k = 0
for el in list(map("".join, product("012345", repeat=5))):
    l = False
    if int(el[0]) == 0: continue
    for i in range(0, len(el) - 1):
        if (int(el[i]) % 2 != 0 and int(el[i + 1]) % 2 != 0) or (int(el[i]) % 2 == 0 and int(el[i + 1]) % 2 == 0):
            l = True
            break
    if not l:
        k += 1
        print(el)
print(k)
